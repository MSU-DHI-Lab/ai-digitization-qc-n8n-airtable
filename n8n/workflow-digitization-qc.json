{
  "name": "Collections Digitization QC Workflow",
  "nodes": [
    {
      "parameters": {
        "path": "/data/incoming",
        "events": ["add"],
        "options": {}
      },
      "name": "Local File Trigger",
      "type": "n8n-nodes-base.localFileTrigger",
      "typeVersion": 1,
      "position": [200, 300]
    },
    {
      "parameters": {
        "keepOnlySet": false,
        "values": {
          "string": [
            {
              "name": "filePath",
              "value": "={{ $binary.data.filePath }}"
            }
          ]
        },
        "options": {}
      },
      "name": "Attach File Path",
      "type": "n8n-nodes-base.set",
      "typeVersion": 2,
      "position": [400, 300]
    },
    {
      "parameters": {
        "conditions": {
          "boolean": [],
          "string": [
            {
              "value1": "={{$json.filePath}}",
              "operation": "notEmpty"
            }
          ]
        }
      },
      "name": "Has File Path?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [600, 300]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "http://model-service:8000/predict",
        "sendBinaryData": true,
        "binaryPropertyName": "data",
        "options": {
          "responseFormat": "json",
          "fullResponse": true,
          "ignoreResponseCode": true
        },
        "headerParametersUi": {
          "parameter": [
            {
              "name": "x-api-token",
              "value": "={{ $env.MODEL_API_TOKEN }}"
            }
          ]
        }
      },
      "name": "AI Model Service (try 1)",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [820, 200]
    },
    {
      "parameters": {
        "conditions": {
          "boolean": [],
          "number": [
            {
              "value1": "={{$json[\"statusCode\"]}}",
              "operation": "equal",
              "value2": 200
            }
          ]
        }
      },
      "name": "API OK? (try 1)",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [1040, 200]
    },
    {
      "parameters": {
        "options": { "unit": "seconds" },
        "interval": 5
      },
      "name": "Retry Delay 1",
      "type": "n8n-nodes-base.wait",
      "typeVersion": 1,
      "position": [1040, 420]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "http://model-service:8000/predict",
        "sendBinaryData": true,
        "binaryPropertyName": "data",
        "options": {
          "responseFormat": "json",
          "fullResponse": true,
          "ignoreResponseCode": true
        },
        "headerParametersUi": {
          "parameter": [
            {
              "name": "x-api-token",
              "value": "={{ $env.MODEL_API_TOKEN }}"
            }
          ]
        }
      },
      "name": "AI Model Service (try 2)",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [1260, 420]
    },
    {
      "parameters": {
        "conditions": {
          "boolean": [],
          "number": [
            {
              "value1": "={{$json[\"statusCode\"]}}",
              "operation": "equal",
              "value2": 200
            }
          ]
        }
      },
      "name": "API OK? (try 2)",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [1480, 420]
    },
    {
      "parameters": {
        "options": { "unit": "seconds" },
        "interval": 10
      },
      "name": "Retry Delay 2",
      "type": "n8n-nodes-base.wait",
      "typeVersion": 1,
      "position": [1480, 640]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "http://model-service:8000/predict",
        "sendBinaryData": true,
        "binaryPropertyName": "data",
        "options": {
          "responseFormat": "json",
          "fullResponse": true,
          "ignoreResponseCode": true
        },
        "headerParametersUi": {
          "parameter": [
            {
              "name": "x-api-token",
              "value": "={{ $env.MODEL_API_TOKEN }}"
            }
          ]
        }
      },
      "name": "AI Model Service (try 3)",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [1700, 640]
    },
    {
      "parameters": {
        "conditions": {
          "boolean": [],
          "number": [
            {
              "value1": "={{$json[\"statusCode\"]}}",
              "operation": "equal",
              "value2": 200
            }
          ]
        }
      },
      "name": "API OK? (try 3)",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [1920, 640]
    },
    {
      "parameters": {
        "conditions": {
          "boolean": [],
          "string": [
            {
              "value1": "={{$json[\"body\"][\"quality\"]}}",
              "operation": "equal",
              "value2": "high"
            }
          ]
        }
      },
      "name": "Quality Router",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [1920, 200]
    },
    {
      "parameters": {
        "operation": "create",
        "baseId": "={{ $env.AIRTABLE_BASE_ID }}",
        "table": "={{ $env.AIRTABLE_TABLE_NAME }}",
        "columns": {
          "mappingMode": "defineBelow",
          "value": {
            "Object ID": "={{ $binary.data.fileName.split('.')[0] }}",
            "File name": "={{ $binary.data.fileName }}",
            "File path or URL": "={{ $binary.data.filePath }}",
            "Status": "QC Passed",
            "Quality": "={{ $json.body.quality }}",
            "Quality score": "={{ $json.body.score }}",
            "Defects (text summary)": "={{ Array.isArray($json.body.reasons) ? $json.body.reasons.join(', ') : '' }}",
            "Finger in frame": "={{ $json.body.defects.finger_in_frame }}",
            "Skew (degrees)": "={{ $json.body.defects.skew_degrees }}",
            "Blur level": "={{ $json.body.defects.blur }}",
            "Glare present": "={{ $json.body.defects.glare }}",
            "Cutoff edges": "={{ $json.body.defects.cutoff_edges }}",
            "AI Analysis Report": "={{ $json.body.rich_text_report }}",
            "Tags": "={{ $json.body.semantic_tags }}"
          }
        }
      },
      "name": "Airtable (passed)",
      "type": "n8n-nodes-base.airtable",
      "typeVersion": 1,
      "position": [2140, 100]
    },
    {
      "parameters": {
        "operation": "create",
        "baseId": "={{ $env.AIRTABLE_BASE_ID }}",
        "table": "={{ $env.AIRTABLE_TABLE_NAME }}",
        "columns": {
          "mappingMode": "defineBelow",
          "value": {
            "Object ID": "={{ $binary.data.fileName.split('.')[0] }}",
            "File name": "={{ $binary.data.fileName }}",
            "File path or URL": "={{ $binary.data.filePath }}",
            "Status": "QC Failed â€“ Needs Review",
            "Quality": "={{ $json.body?.quality || 'low' }}",
            "Quality score": "={{ $json.body?.score || 0 }}",
            "Defects (text summary)": "={{ Array.isArray($json.body?.reasons) ? $json.body.reasons.join(', ') : 'AI call failed (status ' + ($json.statusCode || 'unknown') + ')' }}",
            "Finger in frame": "={{ $json.body?.defects?.finger_in_frame || false }}",
            "Skew (degrees)": "={{ $json.body?.defects?.skew_degrees || 0 }}",
            "Blur level": "={{ $json.body?.defects?.blur || 'unknown' }}",
            "Glare present": "={{ $json.body?.defects?.glare || false }}",
            "Cutoff edges": "={{ $json.body?.defects?.cutoff_edges || false }}",
            "AI Analysis Report": "={{ $json.body?.rich_text_report || 'AI did not return a report. Please review manually.' }}",
            "Tags": "={{ $json.body?.semantic_tags || [] }}"
          }
        }
      },
      "name": "Airtable (failed)",
      "type": "n8n-nodes-base.airtable",
      "typeVersion": 1,
      "position": [2140, 400]
    },
    {
      "parameters": {
        "conditions": {
          "boolean": [],
          "string": [
            {
              "value1": "={{$env.USE_SHELL_MOVES || 'true'}}",
              "operation": "equal",
              "value2": "true"
            }
          ]
        }
      },
      "name": "Allow shell moves? (passed)",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [2360, 80]
    },
    {
      "parameters": {
        "conditions": {
          "boolean": [],
          "string": [
            {
              "value1": "={{$env.USE_SHELL_MOVES || 'true'}}",
              "operation": "equal",
              "value2": "true"
            }
          ]
        }
      },
      "name": "Allow shell moves? (failed)",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [2360, 420]
    },
    {
      "parameters": {
        "command": "mv \"{{$binary.data.filePath}}\" \"/data/processed/\""
      },
      "name": "Move to processed",
      "type": "n8n-nodes-base.executeCommand",
      "typeVersion": 1,
      "position": [2560, 40]
    },
    {
      "parameters": {
        "command": "mv \"{{$binary.data.filePath}}\" \"/data/failed/\""
      },
      "name": "Move to failed",
      "type": "n8n-nodes-base.executeCommand",
      "typeVersion": 1,
      "position": [2560, 380]
    },
    {
      "parameters": {
        "keepOnlySet": false,
        "values": {
          "string": [
            {
              "name": "moveNote",
              "value": "Skipped move (USE_SHELL_MOVES=false)"
            }
          ]
        },
        "options": {}
      },
      "name": "Skip move (passed)",
      "type": "n8n-nodes-base.set",
      "typeVersion": 2,
      "position": [2560, 140]
    },
    {
      "parameters": {
        "keepOnlySet": false,
        "values": {
          "string": [
            {
              "name": "moveNote",
              "value": "Skipped move (USE_SHELL_MOVES=false)"
            }
          ]
        },
        "options": {}
      },
      "name": "Skip move (failed)",
      "type": "n8n-nodes-base.set",
      "typeVersion": 2,
      "position": [2560, 480]
    },
    {
      "parameters": {
        "url": "={{ $env.ALERT_WEBHOOK_URL }}",
        "method": "POST",
        "authentication": "none",
        "jsonParameters": true,
        "bodyParametersJson": "={\"fileName\": $binary.data.fileName, \"filePath\": $binary.data.filePath, \"status\": $json.statusCode || 'unknown', \"message\": 'AI call failed or missing filePath'}",
        "options": {}
      },
      "name": "Notify Failure",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [1700, 820],
      "continueOnFail": true
    },
    {
      "parameters": {
        "keepOnlySet": false,
        "values": {
          "string": [
            {
              "name": "statusCode",
              "value": "500"
            }
          ],
          "json": [
            {
              "name": "body",
              "value": "{ \"quality\": \"low\", \"score\": 0, \"reasons\": [\"File path missing\"], \"defects\": {}, \"semantic_tags\": [], \"rich_text_report\": \"File path missing\" }"
            }
          ]
        },
        "options": {}
      },
      "name": "Set Missing Path Payload",
      "type": "n8n-nodes-base.set",
      "typeVersion": 2,
      "position": [820, 540]
    }
  ],
