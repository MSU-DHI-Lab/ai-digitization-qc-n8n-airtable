      "name": "Local File Trigger",
      "type": "n8n-nodes-base.localFileTrigger",
      "typeVersion": 1,
      "position": [200, 300]
    },
    {
      "parameters": {
        "keepOnlySet": false,
        "values": {
          "string": [
            {
              "name": "filePath",
              "value": "={{ $binary.data.filePath }}"
            },
            {
              "name": "safeFileName",
              "value": "={{ $binary.data.fileName.replace(/[^a-zA-Z0-9._-]/g, '_') }}"
            }
          ]
        },
        "options": {}
      },
      "name": "Attach File Path",
      "type": "n8n-nodes-base.set",
      "typeVersion": 3,
      "position": [400, 300]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": ""
